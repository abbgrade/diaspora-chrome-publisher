{
    "name": "Diaspora* Publisher",
    "description": "__MSG_extDescription__",
    "version": "1.9.6",
    "manifest_version": 2,
    "default_locale": "en",
    "background": {
      "scripts": ["js/jquery.min.js", "js/background.js"]
    },
    "web_accessible_resources": [
      "icon.png"
    ],
    "icons": { "48": "icon.png" },
    "options_page": "options.html",
    "permissions": [
      "tabs", "http://*/*", "https://*/*", "notifications"
    ],
    "browser_action": {
      "default_title": "__MSG_extAction__*",
      "default_icon": "icon.png",
      "default_popup": "share.html"
    }
}
